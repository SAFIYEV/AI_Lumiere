# Groq API key (used only for local dev server / Express server)
GROQ_API_KEY=gsk_your_key_here

# Supabase (used in frontend, exposed via VITE_ prefix)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# API proxy URL (leave empty for local dev, set to Cloudflare Worker URL for production)
# Example: https://ai-lumiere-api.your-account.workers.dev
VITE_API_URL=
